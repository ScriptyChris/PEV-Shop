{
  "name": "fake-pev-shopping",
  "version": "1.0.0",
  "description": "Fake Personal Electric Vehicle Shopping application built with React. Browse, compare, buy, review vehicles and their accessories.",
  "author": "ScriptyChris",
  "license": "ISC",
  "homepage": "https://github.com/ScriptyChris/Fake-PEV-Shopping#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ScriptyChris/Fake-PEV-Shopping.git"
  },
  "keywords": [
    "shopping",
    "pev",
    "react"
  ],
  "bugs": {
    "url": "https://github.com/ScriptyChris/Fake-PEV-Shopping/issues"
  },
  "scripts": {
    "prettier:backend": "prettier --write src/middleware/ src/database/ !src/database/data src/augmentations.d.ts",
    "prettier:test:unit": "prettier --write test/unit __mocks__/** **/__mocks__/**",
    "eslint:backend": "cross-env BACKEND_ONLY=true eslint \"src/middleware/**\" \"src/database/**\"",
    "eslint:test:unit": "cross-env BACKEND_ONLY=true TEST_ENV=true eslint \"test/unit/**\" \"__mocks__/*\" \"src/**/__mocks__/*\"",
    "pretest:unit": "npm run eslint:test:unit && npm run prettier:test:unit",
    "test:unit": "jest --env=node test/unit",
    "test:unit:dev": "jest --watch --env=node test/unit",
    "test:unit:debug": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand",
    "populate-db": "cd src/database/populate && cross-env TS_NODE_PROJECT=../../../tsconfig.backend.json node -r ts-node/register populate.ts productsInputPath=./initial-products.json usersInputPath=./initial-users.json cleanAllBefore",
    "dev:frontend": "webpack-dev-server --env development",
    "dev:backend": "cross-env BACKEND_ONLY=true nodemon --exec \"npm run prebuild:backend && ts-node --files --project tsconfig.backend.json src/middleware/middleware-index.ts\" --watch src/middlware --watch src/database",
    "prebuild:backend": "npm run eslint:backend && npm run prettier:backend",
    "build:backend": "rimraf dist/src/middleware dist/src/database dist/utils && tsc --project tsconfig.backend.json",
    "build:frontend": "rimraf dist/src/frontend/ && webpack --env production",
    "build": "npm run build:backend && npm run build:frontend",
    "debug": "cross-env BACKEND_ONLY=true node --inspect-brk dist/src/middleware/middleware-index.js",
    "serve": "echo '--> ::' && ls ./ && echo '--> UP::' && ls ../ && echo '--> dist/src/middleware/::' && ls dist/src/middleware/ || ls dist/src/ || ls dist/ && node_modules/.bin/cross-env BACKEND_ONLY=true node dist/src/middleware/middleware-index.js"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.12.0",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.10.3",
    "@types/mongoose-paginate-v2": "^1.3.9",
    "@types/node": "^14.14.25",
    "@types/node-fetch": "^2.5.10",
    "@types/nodemailer": "^6.4.4",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "@types/react-paginate": "^7.1.1",
    "@types/react-router-dom": "^5.3.2",
    "@types/uuid": "^8.3.1",
    "@types/webpack": "^4.41.26",
    "@types/webpack-dev-server": "^3.11.1",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "copy-webpack-plugin": "^6.0.4",
    "css-loader": "^4.2.1",
    "dotenv-webpack": "^7.0.2",
    "eslint": "^7.7.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.20.6",
    "fork-ts-checker-webpack-plugin": "^6.4.0",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^4.4.1",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^0.10.0",
    "node-fetch": "^2.6.1",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.4.1",
    "prettier-loader": "^3.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.13.0",
    "react-router-dom": "^5.2.0",
    "sass-loader": "^9.0.3",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.2",
    "formik": "^2.2.9",
    "jsonwebtoken": "^8.5.1",
    "mobx": "^5.15.6",
    "mobx-react-lite": "^2.2.2",
    "mongodb": "3.5.10",
    "mongoose": "^5.9.27",
    "mongoose-paginate-v2": "^1.3.13",
    "mongoose-type-email": "^1.1.2",
    "nodemailer": "^6.7.0",
    "react-paginate": "^7.0.0",
    "react-simple-tree-menu": "^1.1.18"
  }
}
