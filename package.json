{
  "name": "pev-shop",
  "version": "1.0.0",
  "description": "Personal Electric Vehicle Shop application built with React. Browse, compare, buy, review vehicles and their accessories.",
  "author": "ScriptyChris",
  "license": "ISC",
  "homepage": "https://github.com/ScriptyChris/PEV-Shop#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ScriptyChris/PEV-Shop.git"
  },
  "keywords": [
    "shop",
    "pev",
    "react",
    "typescript",
    "demo"
  ],
  "bugs": {
    "url": "https://github.com/ScriptyChris/PEV-Shop/issues"
  },
  "scripts": {
    "prettier:commons": "prettier --write commons/",
    "prettier:backend": "prettier --write src/middleware/ src/database/ !src/database/data src/augmentations.d.ts",
    "prettier:test:unit": "prettier --write test/unit __mocks__/** **/__mocks__/** jest.config.js",
    "prettier:test:e2e": "prettier --write test/e2e/cypress/**/*.ts test/e2e/cypress.json",
    "eslint:commons": "cross-env BACKEND_ONLY=true eslint \"commons/**\"",
    "eslint:backend": "cross-env BACKEND_ONLY=true eslint \"src/middleware/**\" \"src/database/**\"",
    "eslint:test:unit": "cross-env BACKEND_ONLY=true TEST_ENV=true eslint \"test/unit/**\" \"__mocks__/*\" \"src/**/__mocks__/*\"",
    "eslint:test:e2e": "cross-env BACKEND_ONLY=true TEST_ENV=true eslint \"test/e2e/cypress/**/*.ts\"",
    "pretest:unit": "npm run eslint:test:unit && npm run prettier:test:unit",
    "pretest:e2e:dev": "sh ./test/e2e/check-for-essential-deps.sh",
    "pretest:e2e:CI_CD": "test -e .env || cp .env.example .env",
    "test:unit": "jest --env=node test/unit",
    "test:unit:dev": "jest --watch --env=node test/unit",
    "test:unit:debug": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand",
    "test:e2e:dev": "cd ./docker && xhost +local: && sudo docker-compose -f docker-compose.yml -f docker-compose.e2e.yml --env-file ../.env up",
    "test:e2e:CI_CD": "cd ./docker && sudo docker-compose -f docker-compose.yml -f docker-compose.app-standalone-volumes.yml -f docker-compose.e2e.yml -f docker-compose.e2e-ci_cd.yml --env-file ../.env up --abort-on-container-exit",
    "populate-db": "ts-node --files --project tsconfig.backend.json src/database/populate/populate.ts executedFromCLI=true",
    "prepare-commons": "npm run eslint:commons && npm run prettier:commons",
    "dev:frontend": "webpack-dev-server --env development",
    "dev:backend": "cross-env BACKEND_ONLY=true nodemon --config nodemon.backend.json",
    "prebuild:backend": "npm run eslint:backend && npm run prettier:backend",
    "build:backend": "rimraf dist/src/middleware dist/src/database dist/commons && tsc --project tsconfig.backend.json",
    "postbuild:backend": "cp -r src/database/populate/initialData/product-images dist/src/database/populate/initialData/product-images",
    "build:frontend": "rimraf dist/src/frontend/ && webpack --env production",
    "build": "npm run build:backend && npm run build:frontend && npm run prepare-commons",
    "postbuild": "npm run generate-api-docs",
    "debug": "cross-env BACKEND_ONLY=true node --inspect-brk dist/src/middleware/middleware-index.js",
    "update-readme-disclaimer": "node scripts/update-readme-disclaimer.js",
    "generate-api-docs": "node scripts/generate-grouped-api-docs.js",
    "serve": "node_modules/.bin/cross-env BACKEND_ONLY=true node dist/src/middleware/middleware-index.js"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.12.0",
    "@types/bcrypt": "^3.0.0",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.11",
    "@types/formidable": "^2.0.5",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.10.3",
    "@types/mongoose-paginate-v2": "^1.3.9",
    "@types/node": "^14.14.25",
    "@types/node-fetch": "^2.5.10",
    "@types/nodemailer": "^6.4.4",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "@types/react-paginate": "^7.1.1",
    "@types/react-router-dom": "^5.3.2",
    "@types/uuid": "^8.3.1",
    "@types/webpack": "^4.41.26",
    "@types/webpack-dev-server": "^3.11.1",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "@zamiell/typedoc-plugin-not-exported": "^0.2.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "concat-md": "^0.5.1",
    "copy-webpack-plugin": "^6.0.4",
    "css-loader": "^4.2.1",
    "cypress": "^9.1.1",
    "cypress-file-upload": "^5.0.8",
    "dotenv-webpack": "^7.0.2",
    "eslint": "^7.7.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.20.6",
    "fork-ts-checker-webpack-plugin": "^6.4.0",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^4.4.1",
    "jest": "^26.6.3",
    "local-cypress": "^1.2.3",
    "mini-css-extract-plugin": "^0.10.0",
    "node-fetch": "^2.6.7",
    "nodemon": "^2.0.20",
    "prettier": "^2.4.1",
    "prettier-loader": "^3.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.13.0",
    "react-router-dom": "^5.2.0",
    "sass": "^1.58.3",
    "sass-loader": "^9.0.3",
    "style-loader": "^1.3.0",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "typedoc": "^0.22.18",
    "typedoc-plugin-markdown": "^3.12.1",
    "typedoc-plugin-missing-exports": "^0.22.6",
    "typedoc-plugin-rename-defaults": "^0.6.4",
    "typescript": "^4.1.3",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "bcrypt": "^5.0.1",
    "classnames": "^2.3.1",
    "cron": "^2.2.0",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "formidable": "^2.1.1",
    "formik": "^2.2.9",
    "jsonwebtoken": "^8.5.1",
    "mobx": "^5.15.6",
    "mobx-react-lite": "^2.2.2",
    "module-alias": "^2.2.2",
    "mongodb": "3.5.10",
    "mongoose": "5.9.27",
    "mongoose-paginate-v2": "^1.3.13",
    "mongoose-type-email": "^1.1.2",
    "nodemailer": "^6.7.0",
    "query-string": "^7.1.1",
    "react-paginate": "^7.0.0",
    "react-simple-tree-menu": "^1.1.18"
  }
}
