{
  "extends": "./tsconfig.json",
  "include": ["./test/e2e/cypress/**/*.ts"], // ["./**/*.ts"]
  // "exclude": ["./fixtures/src"]
  "compilerOptions": {
    // "target": "es2020",
    "module": "ES2020", //"commonjs",
    "lib": ["es5", "ES2015", "dom"],
    "baseUrl": ".",
    "paths": {
      "@srcForE2E/*": ["src/*" /* , "./fixtures/src" */]
    },
    // "rootDirs": ["../../../src", "./fixtures"],
    // "typeRoots": [
    //   //   // official Cypress Docker container keeps types there
    //   //   // "/usr/local/lib/node_modules/cypress/types"
    //   "test/e2e/node_modules/cypress/types"
    //   // "node_modules/@types"
    // ],
    "types": ["cypress" /* , "mongoose" */ /* , "mongodb" */],
    "allowJs": true,
    "resolveJsonModule": true,
    "listEmittedFiles": true
  }
}
